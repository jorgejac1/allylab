    "@fastify/swagger-ui": "^5.2.5",
    "axe-core": "^4.11.1",
    "dotenv": "^17.2.3",
    "fastify": "^5.7.2",
    "pino": "^10.3.0",
    "playwright": "^1.58.1",
    "prom-client": "^15.1.3",
    "zod": "^4.3.6"
